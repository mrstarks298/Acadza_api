{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "functions": {
    "api/generate-pdf.js": {
      "maxDuration": 30,
      "memory": 1024
    },
    "app/api/**/*": {
      "maxDuration": 30,
      "memory": 1024
    }
  },
  "rewrites": [
    {
      "source": "/",
      "destination": "/index.html"
    }
  ]
}
